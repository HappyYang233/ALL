import java.io.File;
import java.io.IOException;

public class demo1 {
	 public static void main(String[] args)
	 {
		 File f= new File("./text.txt");
		 if(!f.exists()) {
		 try {
			f.createNewFile();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		 
		 }
		 else
		 {
			 f.delete();
		 }
	 }
}
