class Father
{
	public Father(int i) {
		this.p=i;
		System.out.println("fa");
	}
		protected int k = 0;
		private int p = 1;
		public int getP() {
			return p;
		}
		public void setP(int p) {
			this.p = p;
		}
		private final int q = 100;
		public void FMethod(){}
}

class Son extends Father {
	private static int i;
		public Son()
		{
			super(i);
			System.out.println("son");
		}
		}
