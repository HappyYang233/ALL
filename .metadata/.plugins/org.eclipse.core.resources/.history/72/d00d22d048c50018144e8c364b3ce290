package 继承的认知;

public class Main {
	
	
	public static void main(String[] args)
	{
	Father f = new Son();
	Son s = new Son();
		  f.FMethod();
	s.FMethod();
	}
		}
		 
class Father{
		protected int k = 0;
		private int p = 1;
		private final int q = 100;
		public void FMethod(){}
		}

class Son extends Father {
		}
