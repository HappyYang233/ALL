package 继承的认知;

public class Main {	
	public static void main(String[] args)
	{
	Son s = new Son();	  
	}
}
		 
class Father
{
	public Father(int i) {
		this.p=i;
		System.out.println("fa");
	}
		
		private int p ;
		
	
}

class Son extends Father {
	private static int i;
		public Son()
		{
			super(i);
			System.out.println("son");
		}
		}
